<?xml version="1.0" encoding="UTF-8"?>
<!--
  iOS Privacy Manifest for VibeFit

  NOTE: This file is placed at the project root because this is an Expo managed
  workflow without an ios/ directory. To include it in iOS builds, add it via an
  Expo config plugin or copy it to the ios/ directory after running `expo prebuild`.

  Example app.json plugin config:
  {
    "expo": {
      "plugins": [
        ["expo-build-properties", {
          "ios": {
            "privacyManifests": {
              "NSPrivacyAccessedAPITypes": [...],
              "NSPrivacyCollectedDataTypes": [...]
            }
          }
        }]
      ]
    }
  }
-->
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
